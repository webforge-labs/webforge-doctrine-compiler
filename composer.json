{
  "name":"webforge/doctrine-compiler",
  "description":"Generate your doctrine entities metadata from a simple json file, including the php code for the entity",
  "require":{
    "webforge/doctrine":"1.2.*@dev",
    "webforge/webforge":"1.7.*@dev"
  },
  "license":"MIT",
  "authors":[
    {
      "name":"Philipp Scheit",
      "email":"p.scheit@ps-webforge.com"
    }
  ],
  "minimum-stability":"dev",
  "autoload":{
    "psr-0":{
      "Webforge\\Doctrine\\Compiler":["lib/", "tests/"]
    }
  },
  "require-dev": {
    "satooshi/php-coveralls": "dev-master",
    "webforge/testplate": "1.*"
  },
  "extra":{
    "branch-alias":{
      "dev-master":"1.0.x-dev"
    }
  }
}
